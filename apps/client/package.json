{
  "name": "pokemon-client",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/nkzw-tech/web-app-template.git"
  },
  "author": "Christoph Nakazawa <christoph.pojer@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "relay-compiler --validate && next build --turbopack",
    "check": "npm run tc && npm run lint",
    "dev": "mkdir -p src/__generated__ && concurrently \"relay-compiler --watch\" \"next dev --turbopack\"",
    "format": "biome format --write .",
    "lint": "biome lint src --write",
    "relay": "mkdir -p src/__generated__ && relay-compiler"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "@nkzw/core": "^1.3.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-relay": "^20.1.1",
    "relay-runtime": "^20.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "relay-compiler": "^20.1.1",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.0.0"
  }
}
